{
  "name": "@talkohavy/lodash",
  "version": "1.0.46",
  "description": "A bunch of lodash-like methods",
  "private": "true",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup --config tsup.config.js",
    "lint": "eslint -c ./.eslintrc.json \"./src/**/*.js\"",
    "test": "node --test",
    "format": "prettier . --write",
    "check-format": "prettier . --check",
    "preversion": "npm test",
    "version": "npm run format && npm run build",
    "postversion": "git push && git push --tags && cd dist && npm publish",
    "typecheck": "tsc -p jsconfig.json && cp package.json dist/package.json",
    "prepare": "husky install"
  },
  "sideEffects": false,
  "contributors": [
    {
      "name": "Tal Kohavy",
      "email": "talkohavy@gmail.com",
      "url": "https://github.com/talkohavy"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/talkohavy/lodash"
  },
  "author": "Tal Kohavy",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "restricted"
  },
  "bugs": {
    "url": "https://github.com/talkohavy/lodash/issues",
    "email": "talkohavy@gmail.com"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "eslint": "^8.54.0",
    "eslint-plugin-ezlint": "^1.0.25",
    "husky": "^8.0.3",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
