{
  "name": "@talkohavy/lodash",
  "version": "1.0.38",
  "description": "A bunch of lodash-like methods",
  "private": "true",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup --config tsup.config.js",
    "lint": "eslint -c ./.eslintrc.json \"./lib/**/*.js\"",
    "test": "node --test",
    "format": "prettier --write .",
    "preversion": "npm test",
    "version": "npm run format && npm run build",
    "postversion": "git push && git push --tags && cd dist && npm publish",
    "prepare": "husky install"
  },
  "sideEffects": false,
  "contributors": ["Tal Kohavy <talkohavy@gmail.com> (@Tal Kohavy)"],
  "repository": {
    "type": "git",
    "url": "https://github.com/talkohavy/lodash"
  },
  "author": "Tal Kohavy",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "restricted"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.54.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-sort-exports": "^0.8.0",
    "husky": "^8.0.3",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  }
}
